{
  "name": "eo-mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "emulator": "~/Library/Android/sdk/emulator/emulator -avd Pixel_7_Pro",
    "emulator:swiftshader": "~/Library/Android/sdk/emulator/emulator -avd Pixel_7_Pro -gpu swiftshader-indirect",
    "emulator:3a": "~/Library/Android/sdk/emulator/emulator -avd Pixel_3a_API_33_arm64-v8a",
    "emulator:update": "echo 'To update the emulator, please use Android Studio: Tools > SDK Manager > SDK Tools > Android Emulator > Apply'",
    "postinstall": "cp node_modules/react-native-leaflet-view/android/src/main/assets/leaflet.html assets/leaflet.html 2>/dev/null || echo 'Note: leaflet.html will be copied after package installation'"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/native": "^7.1.8",
    "axios": "^1.13.4",
    "expo": "~54.0.32",
    "expo-asset": "^12.0.12",
    "expo-constants": "~18.0.13",
    "expo-file-system": "^19.0.21",
    "expo-font": "~14.0.11",
    "expo-image-picker": "^17.0.10",
    "expo-linking": "~8.0.11",
    "expo-location": "~18.0.4",
    "expo-router": "~6.0.22",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-leaflet-view": "^1.0.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
